{
    "rules": {
	    "no-console": 1,
	    "no-var": 1,
	    "object-shorthand": 1,
	    "no-debugger": 2,
	    "no-sparse-arrays": 2,
	    "no-inner-declarations": 2,
	    "no-invalid-regexp": 2,
	    "no-const-assign": 2,
	    "no-dupe-args": 2,
	    "no-dupe-keys": 2,
	    "no-class-assign": 2,
	    "require-yield": 2,
	    "no-extra-semi": 2,
	    "no-func-assign": 2,
	    "no-extra-parens": 2,
	    "no-duplicate-case": 2,
	    "no-mixed-spaces-and-tabs": 2,
	    "no-multiple-empty-lines": 2,
	    "no-new-object": 2,
	    "no-loop-func": 2,
	    "generator-star-spacing": [2, {"before": true, "after": false}],
	    "valid-typeof": 2,
	    "no-implicit-coercion": 2,
	    "dot-notation": 2,
	    "default-case": 2,
	    "eqeqeq": 2,
	    "arrow-spacing": 2,
	    "arrow-parens": 2,
	    "comma-dangle": 0,
	    "no-cond-assign": 2,
	    "use-isnan": 2,
	    "no-spaced-func": 2,
	    "no-nested-ternary": 2,
	    "object-curly-spacing": 0,
	    "block-spacing": 2,
        "indent": [2, "tab"],
        "quotes": [2, "single"],
        "brace-style": 2,
        "camelcase": 2,
        "linebreak-style": [2, "unix"],
        "no-unused-vars": [1, {"vars" : "all", "args" : "after-used"}],
        "comma-spacing": [2, {"before": false, "after": true}],
        "computed-property-spacing": [2, "never"],
        "consistent-this": [2, "_this"],
        "semi-spacing": 2,
        "semi": [2, "always"]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
		"mocha": true
    },
    "extends": "eslint:recommended"
}